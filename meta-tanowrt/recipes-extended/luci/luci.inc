
LUCI_GIT_URI      ?= "git://github.com/tano-systems/luci.git"
LUCI_GIT_BRANCH   ?= "tano/master"
LUCI_GIT_PROTOCOL ?= "git"

# 22.01.2020
LUCI_GIT_SRCREV ?= "56ae5314d347fa9a80fffdc1de9e457c623580e0"

PR_append = ".41"

libdir = "/usr/lib"

# Default distributive and LuCI name and version
