
LUCI_GIT_URI      ?= "git://github.com/tano-systems/luci.git"
LUCI_GIT_BRANCH   ?= "tano/master"
LUCI_GIT_PROTOCOL ?= "git"

# 13.02.2018
LUCI_GIT_SRCREV ?= "899e730939da4ea6e80684f254e41a1400088781"

PR_append = ".23"

libdir = "/usr/lib"

