	option port_swp5_adminedge     'no'
	option port_swp5_autoedge      'yes'
	option port_swp5_p2p           'auto'
	option port_swp5_bpduguard     'no'
	option port_swp5_restrrole     'no'
	option port_swp5_restrtcn      'no'
	option port_swp5_treeprio      '8'
